{
  "component": "antd:Form",
  "props": {
    "title": "Login"
  },
  "children": [
    {
      "component": "a10:FormItem",
      "datasource": {
        "source": "credentials:username",
        "defaultvalue": "admin",
        "autoload": false
      },
      "props": {
        "type": "antd:Input"
      }
    },
    {
      "component": "a10:FormItem",
      "datasource": {
        "source": "credentials:password",
        "defaultvalue": "admin",
        "autoload": false
      },

      "defaultvalue": "a10",
      "props": {
        "type": "antd:Input.Password"
      }
    }
  ]
}
