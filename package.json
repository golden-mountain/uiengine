{
  "name": "UIEngine",
  "version": "1.0.0",
  "description": "JSON Schema based extendable UI Engine",
  "main": "lib/UIEngine.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require ts-node/register --colors ./test/*.spec.ts",
    "test:watch": "mocha --require ts-node/register --colors --watch-extensions ts,tsx -w ./test/*.spec.ts",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require ts-node/register --colors test/*.ts",
    "repl": "node -i -e \"$(< ./lib/UIEngine.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zuopingli/UIEngine.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Zuoping Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zuopingli/UIEngine/issues"
  },
  "homepage": "https://github.com/zuopingli/UIEngine",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3",
    "source-map-loader": "^0.2.4",
    "awesome-typescript-loader": "^5.2.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.1"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "csstype": "^2.6.5",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
