{
  "id": "state-node-basic",
  "component": "lib:DemoElement1",
  "datasource": "foo:bar",
  "children": [
    {
      "component": "lib:DemoElement2",
      "id": "id-of-demo-element-1",
      "datasource": "foo:bar.name",
      "props": {
        "name": "demo-element-2"
      }
    },
    {
      "component": "lib:DemoLiveElement",
      "datasource": "foo:bar.baz",
      "state": {
        "deps": {
          "visible": [
            {
              "prop": "id",
              "id": "id-of-demo-element-1",
              "state": {
                "visible": true
              }
            }
          ]
        }
      },
      "$children": [
        {
          "component": "p",
          "datasource": "foo:bar.baz.$.name",
          "id": "for.bar.baz.$.name"
        },
        {
          "component": "p",
          "datasource": "foo:bar.baz.$.age",
          "state": {
            "deps": {
              "visible": [
                {
                  "prop": "id",
                  "id": "for.bar.baz.$.name",
                  "data": "Rui"
                }
              ]
            }
          }
        }
      ],
      "props": {
        "defaultvalue": "hello"
      }
    }
  ]
}
