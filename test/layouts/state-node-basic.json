{
  "id": "state-node-basic",
  "component": "lib:DemoElement1",
  "datasource": "foo:bar",
  "children": [
    {
      "component": "lib:DemoElement2",
      "id": "id-of-demo-element-1",
      "datasource": "foo:bar.name",
      "props": {
        "name": "demo-element-2"
      }
    },
    {
      "component": "lib:DemoLiveElement",
      "datasource": "foo:bar.baz",
      "state": {
        "visible": {
          "strategy": "and",
          "deps": [
            {
              "selector": {
                "id": "id-of-demo-element-1"
              },
              "state": {
                "visible": true
              }
            }
          ]
        }
      },
      "$children": [
        {
          "component": "p",
          "datasource": "foo:bar.baz.$.name",
          "id": "for.bar.baz.$.name"
        },
        {
          "component": "p",
          "datasource": "foo:bar.baz.$.age",
          "state": {
            "visible": {
              "deps": [
                {
                  "selector": {
                    "id": "for.bar.baz.$.name"
                  },
                  "data": ""
                }
              ]
            }
          }
        }
      ],
      "props": {
        "defaultvalue": "hello"
      }
    }
  ]
}
